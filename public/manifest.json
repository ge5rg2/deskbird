{
  "manifest_version": 3,
  "name": "DeskBird",
  "version": "1.0",
  "version_name": "1.0 beta",
  "description": "A chrome extension that shows nearby birds information.",
  "permissions": ["geolocation"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icons/p_icon.png"
    },
    "default_locale": "en"
  },
  "icons": {
    "128": "icons/p_icon.png"
  }
}
